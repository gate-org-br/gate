	public void update(${name} value) throws AppException
	{
		if (Update.table("${tableName}")
			<g-iterator source="${properties}" target="property">
			.set("${@property.columnName}", value.${@property.getter})
			</g-iterator>
			.where(Condition.of("${id.columnName}").eq(value.${id.getter}))
			.build()
			.connect(getLink())
			.execute() == 0)
			throw new NotFoundException();
	}