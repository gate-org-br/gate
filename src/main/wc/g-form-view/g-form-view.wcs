* {
	box-sizing: border-box
}

:host(*) {
	display: flex;
	align-items: stretch;
	flex-direction: column;
	justify-content: stretch;
}

fieldset {
	padding: 0;
	border: none;
}

span.multiple
{
	padding: 4px;
	text-indent: 0;
	overflow: auto;
	line-height: 24px;
	flex-basis: 128px;
	white-space: pre-line;
}