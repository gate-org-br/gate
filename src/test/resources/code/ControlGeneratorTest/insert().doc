	public void insert(User value) throws ConstraintViolationException
	{
		Constraints.validate(value,
			"active",
			"userID",
			"passwd",
			"name",
			"email",
			"role.id",
			"details",
			"cellPhone",
			"phone",
			"photo",
			"birthdate",
			"CPF",
			"sex",
			"registration");

		try (UserDao dao = new UserDao())
		{
			dao.insert(value);
		}
	}