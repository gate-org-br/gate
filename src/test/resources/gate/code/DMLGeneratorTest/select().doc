select 
    Uzer.id as 'id',
    Uzer.active as 'active',
    Uzer.username as 'username',
    Uzer.password as 'password',
    Uzer.name as 'name',
    Uzer.email as 'email',
    Uzer.Role$id as 'role.id',
    Uzer.description as 'description',
    Uzer.cellPhone as 'cellPhone',
    Uzer.phone as 'phone',
    Uzer.photo as 'photo',
    Uzer.birthdate as 'birthdate',
    Uzer.CPF as 'CPF',
    Uzer.sex as 'sex',
    Uzer.registration as 'registration',
    Uzer.code as 'code'
from
    Uzer
