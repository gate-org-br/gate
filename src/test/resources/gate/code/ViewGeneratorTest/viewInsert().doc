

<g:template filename="/WEB-INF/views/DIAG.jsp">
	<form method='POST' action='#' enctype="multipart/form-data">
		<fieldset>
			<legend>
				Novo Usuário<g:icon type="insert"/>
			</legend>

			<label>
				Ativo:
				<span>
					<g:select property="form.active" tabindex="1"/>
				</span>
			</label>
			<label>
				Login do Usuário:
				<span>
					<g:text property="form.username" tabindex="1"/>
				</span>
			</label>
			<label>
				Senha do Usuário:
				<span>
					<g:text property="form.password" tabindex="1"/>
				</span>
			</label>
			<label>
				Nome do Usuário:
				<span>
					<g:text property="form.name" tabindex="1"/>
				</span>
			</label>
			<label>
				E-Mail do Usuário:
				<span>
					<g:text property="form.email" tabindex="1"/>
				</span>
			</label>
			<label>
				Perfil:
				<span>
					<g:text property="form.role.id" tabindex="1"/>
				</span>
			</label>
			<label>
				Comentários sobre o Usuário:
				<span>
					<g:text property="form.description" tabindex="1"/>
				</span>
			</label>
			<label>
				Telefone Celular do Usuário:
				<span>
					<g:text property="form.cellPhone" tabindex="1"/>
				</span>
			</label>
			<label>
				Telefone Fixo do Usuário:
				<span>
					<g:text property="form.phone" tabindex="1"/>
				</span>
			</label>
			<label>
				Foto do Usuário:
				<span>
					<g:file property="form.photo" tabindex="1"/>
				</span>
			</label>
			<label>
				Data de Nascimento:
				<span>
					<g:text property="form.birthdate" tabindex="1"/>
				</span>
			</label>
			<label>
				CPF:
				<span>
					<g:text property="form.CPF" tabindex="1"/>
				</span>
			</label>
			<label>
				Sexo:
				<span>
					<g:select property="form.sex" tabindex="1"/>
				</span>
			</label>
			<label>
				Data de Cadastro:
				<span>
					<g:text property="form.registration" tabindex="1"/>
				</span>
			</label>
			<label>
				Código do usuário:
				<span>
					<g:text property="form.code" tabindex="1"/>
				</span>
			</label>
		</fieldset>

		<div class='Coolbar'>
			<g:link class="Commit" method="post" module="#" screen="#" action="#" tabindex='2'>
				Concluir<g:icon type="commit"/>
			</g:link>
			<a class='Cancel Hide' tabindex='2'>
				Desistir<g:icon type='cancel'/>
			</a>
		</div>
	</form>
</g:template>