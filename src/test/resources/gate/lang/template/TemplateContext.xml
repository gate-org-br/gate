<html>
	<body>
		<g-if condition='${not empty roles}'>
			<ul>
				<g-for each='${roles}'>
					<li>
						${name}
						<g-if condition='${not empty users}'>
							<ul>
								<g-for each='${users}'>
									<li>
										${name} (${^name})
									</li>	
								</g-for>
							</ul>
						</g-if>
					</li>	
				</g-for>
			</ul>
		</g-if>
	</body>
</html>
